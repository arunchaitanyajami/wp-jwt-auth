{
  "name": "arunchaitanyajami/wp-api-jwt-auth",
  "description": "A simple plugin to add JSON Web Token (JWT) Authentication to WP REST API.",
  "homepage": "https://github.com/arunchaitanyajami/wp-jwt-auth/",
  "type": "wordpress-plugin",
  "minimum-stability": "stable",
  "prefer-stable" : true,
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Arun Chaitanya Jami",
      "homepage": "https://github.com/arunchaitanyajami"
    }
  ],
  "support": {
    "issues": "https://github.com/arunchaitanyajami/wp-jwt-auth/issues",
    "source": "https://github.com/arunchaitanyajami/wp-jwt-auth/"
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:arunchaitanyajami/wp-jwt-auth.git"
    }
  ],
  "require": {
    "php": ">=7.0",
    "firebase/php-jwt": "^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "AJ\\": [
        "includes/",
        "pointer/"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.2",
    "guzzlehttp/guzzle": "^6.1"
  }
}
